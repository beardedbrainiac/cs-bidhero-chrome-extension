{
    "manifest_version": 3,
    "name": "Cloudstaff BidHero Extension",
    "description": "Picks the lowest bid for the item of your choice",
    "version": "1.0",
    "icons": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-64.png",
      "128": "images/icon-128.png"
    },
    "content_scripts": [
      {
        "js": [
          "scripts/content.js"
        ],
        "matches": [
          "https://bidhero.cloudstaff.com/"
        ]
      }
    ]
  }